pipeline {
    agent { docker { image 'maven:3.3.3' } }
       stages {
        stage ('Compile and Test Stage') {

            steps {
                withMaven(maven : 'maven-3.1.0') {
                    sh 'mvn clean install'
                }
            }
        }
        
        stage ('Deployment Stage') {
            steps {
                withMaven(maven : 'maven-3.1.0') {
                    sh 'mvn deploy'
                }
            }
        }
    }
}
